                    ${L`O`A`de`DmoDULES} = ${TaRg`EtPrOC`E`SS}."M`O`dULeS"

                    ${pR`OCE`SSOwNer} = ${OW`NE`Rs}[${tarGeTpr`o`Ce`SS}."Id"."T`o`STRinG"()]

                    ForEach (${M`Od`Ule} in ${L`oaDe`D`m`oDuLeS}){

                        ${mo`DuL`EPath} = "$BasePath\$($Module.ModuleName)"

                        
                        if ((-not ${M`OD`U`lEPaTH}."co`NtA`Ins"(((("{5}{4}{1}{0}{6}{2}{3}" -f 's','}Sy','m3','2','indows{0','C:{0}W','te'))-f  [CHar]92))) -and (-not (&('Tes'+'t-P'+'ath') -Path ${M`ODUle`path})) -and (${KNO`W`NDlLS} -NotContains ${m`Od`ULe}."MoDU`lEna`mE")) {

                            ${e`xClU`De} = ${f`Alse}

                            if(${P`Sbou`NDPA`RaMETERS}[("{2}{3}{1}{0}" -f 'ws','eWindo','Exclu','d')] -and ${modul`E`pa`Th}."c`On`TaINs"(((("{1}{2}{3}{0}"-f'ows','C:fE6W','in','d'))."R`EPl`ACE"(([char]102+[char]69+[char]54),'\')))) {
