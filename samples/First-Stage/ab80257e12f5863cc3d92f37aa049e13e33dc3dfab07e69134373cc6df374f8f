function GET-`MOd`IFI`AbLESEr`VICE {

    [CmdletBinding()] param()

    &('G'+'et-Servic'+'e') | &('T'+'est-'+'Ser'+'viceDac'+'lPermi'+'ssion') -PermissionSet ("{0}{2}{1}" -f'Change','nfig','Co') | &('F'+'or'+'Eac'+'h-Object') {

        ${servIcE`d`Eta`iLS} = ${_} | &('Get'+'-Servi'+'ceDetai'+'l')

        ${s`E`RvICERE`sTArT} = ${_} | &('Test-Ser'+'v'+'i'+'ce'+'DaclPe'+'rmission') -PermissionSet ("{0}{2}{1}"-f'Res','t','tar')

        if(${SerVi`Ce`R`ESt`Art}) {
            ${cAnR`EST`ArT} = ${t`Rue}
        }
        else {
            ${C`An`REstARt} = ${fA`LSe}
        }

        ${O`UT} = &('N'+'ew-O'+'bje'+'ct') p`sOBjE`ct
        ${o`Ut} | &('Ad'+'d-Me'+'mber') notepro`pe`R`TY ("{0}{3}{1}{2}"-f'Servi','Nam','e','ce') ${s`E`Rv`iCEdEtAILs}."na`me"
        ${O`UT} | &('Add-Me'+'mbe'+'r') no`TE`p`ROPeRtY ("{0}{1}"-f 'P','ath') ${S`E`R`V`icEdeTAilS}."PatHn`AMe"
        ${o`Ut} | &('Add'+'-Membe'+'r') nO`TE`P`ROpErty ("{0}{2}{1}" -f'Sta','Name','rt') ${SE`Rv`Iced`E`TAIlS}."sta`R`TnaMe"
        ${o`Ut} | &('Add-M'+'e'+'m'+'ber') NOtE`P`RoPe`RTy ("{0}{1}{2}{3}"-f 'Abus','eFu','nct','ion') "Invoke-ServiceAbuse -Name '$($ServiceDetails.name)' "
        ${O`UT} | &('Add-'+'Mem'+'ber') n`O`TepRopeR`Ty ("{0}{1}{2}" -f 'Can','R','estart') ${CA`NREs`TA`RT}
        ${o`Ut}
    }
}


