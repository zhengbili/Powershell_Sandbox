        ${kN`owNDl`Ls} = $(ForEach (${k`EYnA`ME} in ${k`EYS}."GeT`Val`UENames"()) { ${Ke`YS}."geTV`ALUe"(${KeY`NA`me}) }) | &('Wh'+'e'+'r'+'e-Object') { ${_}."e`Nd`SwitH"(("{1}{0}"-f 'l','.dl')) }
        ${c`UrR`ENT`User} =   ${q`Tl}::"Ge`Tc`UR`Rent"()."NA`me"

        
        ${Ow`NerS} = @{}
        &('Ge'+'t-W'+'miO'+'bject') -Class wi`N32_PR`oCEss | &('Whe'+'re-Objec'+'t') {${_}} | &('Fo'+'rEa'+'ch-Objec'+'t') { ${Own`Ers}[${_}."han`dle"] = ${_}."GEto`Wn`ER"()."US`Er" }
    }

    PROCESS {

        ForEach (${PrOCeS`sn`A`Me} in ${N`Ame}) {

            ${t`ARGe`TPrOce`ss} = &('Get-P'+'r'+'oc'+'ess') -Name ${P`RoCes`SnA`Me}

