            ("{1}{0}"-f'rt','Sta')                 = [uint32]("{1}{2}{0}" -f'10','0x00000','0')
            ("{0}{1}" -f 'St','op')                  = [uint32]("{0}{1}{2}"-f'0x','0','0000020')
            ("{2}{1}{0}"-f 'ntinue','eCo','Paus')         = [uint32]("{3}{0}{1}{2}" -f 'x0','0000','040','0')
            ("{1}{0}{2}"-f 'o','Interr','gate')           = [uint32]("{0}{3}{1}{2}" -f'0x000','008','0','0')
            ("{4}{1}{0}{2}{3}"-f 'edCo','Defin','ntr','ol','User')    = [uint32]("{0}{3}{2}{1}"-f'0','0000100','0','x')
            ("{1}{0}" -f 'te','Dele')                = [uint32]("{3}{0}{1}{2}" -f 'x','000','10000','0')
            ("{3}{2}{0}{1}"-f 'ntro','l','adCo','Re')           = [uint32]("{1}{0}{2}"-f '002000','0x0','0')
            ("{2}{1}{0}" -f 'Dac','e','Writ')              = [uint32]("{3}{1}{2}{0}"-f'000','004','0','0x0')
            ("{2}{1}{0}"-f'er','n','WriteOw')            = [uint32]("{0}{2}{1}"-f'0x','00','000800')
            ("{1}{0}{2}"-f'ro','Synch','nize')           = [uint32]("{0}{2}{1}" -f'0x00','00','1000')
            ("{3}{2}{0}{1}" -f 'Secu','rity','m','AccessSyste')  = [uint32]("{0}{1}{2}"-f '0x','0100','0000')
            ("{1}{0}{2}" -f 'eneric','G','All')            = [uint32]("{1}{0}{2}" -f'0','0x10','00000')
            ("{1}{2}{0}"-f'cExecute','Gener','i')        = [uint32]("{1}{3}{0}{2}"-f'200000','0','00','x')
            ("{1}{0}{2}{3}"-f'ene','G','ric','Write')          = [uint32]("{2}{1}{0}" -f'000','0','0x4000')
            ("{2}{0}{1}"-f'ea','d','GenericR')           = [uint32]("{2}{1}{0}"-f'00000','800','0x')
            ("{2}{1}{0}"-f'ss','Acce','All')             = [uint32]("{0}{2}{1}" -f '0x00','FF','0F01')
        }

        ${che`ck`AlL`P`ERMiSSI`OnS`InseT} = ${f`ALSe}

        if(${P`sBOuNDpAR`Ame`TerS}[("{1}{2}{3}{0}" -f 's','Perm','i','ssion')]) {
            ${tAr`GeTp`ERmISs`IONS} = ${pEr`mISsio`Ns}
        }
        else {
            if(${P`ERMIs`SIOnSET} -eq ("{2}{1}{3}{0}" -f'ig','hangeCon','C','f')) {
                ${TARGET`p`E`Rmiss`ionS} = @(("{1}{2}{0}{3}" -f 'n','Cha','ngeCo','fig'), ("{0}{1}" -f'Writ','eDac'), ("{0}{2}{1}" -f 'WriteOw','r','ne'), ("{2}{1}{0}{3}" -f 'neri','e','G','cAll'), ("{3}{0}{1}{2}"-f'Generic','Writ','e',' '), ("{1}{0}{2}"-f 'cces','AllA','s'))
            }
            elseif(${pe`RM`iS`sIonsET} -eq ("{1}{0}" -f'estart','R')) {
                ${Ta`RgeT`P`Erm`iSsIOnS} = @(("{1}{0}"-f't','Star'), ("{1}{0}" -f'top','S'))
                ${CHEcK`A`llPERMIs`Sions`iNSEt} = ${t`RuE} 
            }
            elseif(${PE`Rm`IssiONs`Et} -eq ("{2}{3}{1}{0}" -f 's','s','AllAc','ce')) {
                ${tARGetP`E`R`m`i`SSioNS} = @(("{0}{2}{3}{1}"-f 'Ge','All','n','eric'), ("{0}{2}{1}" -f 'AllA','s','cces'))
            }
        }
    }

    PROCESS {

        ForEach(${in`D`IvI`DUAL`S`ErViCE} in ${NA`mE}) {

            ${TArGet`SEr`VICe} = ${IndiVID`Ua`lSerVi`ce} | &('Add-Servi'+'c'+'eDa'+'cl')

            if(${tA`R`GEtSERvi`Ce} -and ${TAr`GEtsER`ViCe}."da`CL") {

                
                ${uSERi`d`EN`TitY} =   ( &('gi') ("VARIAbL"+"e"+":Qt"+"l") )."val`Ue"::"GE`Tc`URR`ENt"()
                ${C`URrenTuS`Er`sIdS} = ${u`seRid`EnTiTY}."g`Ro`Ups" | &('S'+'elect-'+'Objec'+'t') -ExpandProperty vAL`UE
                ${cU`RR`EntuSe`R`S`iDs} += ${USERiD`ENti`Ty}."u`sER"."Va`LUe"

                ForEach(${SE`RvI`c`Edacl} in ${TArGet`sERv`i`Ce}."Da`Cl") {
                    if(${CU`RreN`T`U`s`ERsIdS} -contains ${sE`RvI`C`EdacL}."SEcUrIt`YI`DeNti`F`IER") {

                        if(${Che`CkAll`pErm`i`ssIOnsI`NSET}) {
                            ${aLl`mA`T`CheD} = ${tR`Ue}
                            ForEach(${Ta`Rg`ETPerMIsSi`On} in ${TargetP`ERMiSS`Io`NS}) {
                                
                                if ((${Serv`i`C`EdACl}."acC`EsS`RiG`htS" -band ${a`C`c`EssmAsk}[${TarGetPEr`MIS`S`iOn}]) -ne ${ACcEsS`ma`sK}[${tARGet`PErmIss`i`ON}]) {
                                    
