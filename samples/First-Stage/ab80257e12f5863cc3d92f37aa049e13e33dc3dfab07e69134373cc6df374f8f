                if ((-not ${reS`ULt}) -and (${LastEr`R`or} -eq 122) -and (${sI`zE`Ne`EDed} -gt 0)) {
                    ${bi`Na`RySecUrITYDEsCRI`p`ToR} = &('Ne'+'w-'+'Object') bY`T`E[](${SIZ`EnEEd`Ed})

                    ${R`EsU`lT} = ${Ad`VAp`i32}::"quERYs`E`RviCeobJECt`se`C`UrITY"(${sE`RviCe`Ha`N`DLE},   (&('D'+'Ir')  ("var"+"i"+"A"+"BlE:Ctfh"+"9")  )."va`lue"::"DiScr`E`Ti`oNARy`ACL", ${BIN`ARY`secU`Ri`Ty`desc`R`ipTOr}, ${BINaRYS`EC`U`RITy`desCRIpT`oR}."Cou`Nt", [Ref] ${S`IZ`E`NEedEd});${LaST`Er`ROr} =   (&('GeT'+'-'+'VArIabLe') ('3'+'45xY') )."VAL`Ue"::"GeTLAStwI`N3`2ErR`or"()

                    if (-not ${Res`U`LT}) {
                        &('W'+'rite-E'+'rro'+'r') ([ComponentModel.Win32Exception] ${LAS`T`Er`Ror})
                    }
                    else {
                        ${raws`ECu`R`ITy`De`scrIpT`or} = &('Ne'+'w-O'+'bject') seCU`RITy.A`CC`E`sScOnTr`Ol.ra`WS`E`c`URIty`DE`ScrI`ptor -ArgumentList ${BInA`RYSE`cur`itYdESC`RIp`TOR}, 0
                        ${Da`cl} = ${R`A`WSE`CURiT`YDe`sCR`iPTOR}."DiSCr`E`T`iONaryacL" | &('Fo'+'rEach-'+'Objec'+'t') {
                            &('Add-M'+'emb'+'er') -InputObject ${_} -MemberType nO`TE`PrOPer`TY -Name aCcES`s`RIG`Hts -Value (${_}."ACc`ESSma`Sk" -as ${S`Erv`ICEaCCE`SSRIghTS}) -PassThru
                        }

                        &('Add-M'+'e'+'mber') -InputObject ${IN`di`Vi`DuAls`ER`VIcE} -MemberType n`OTepr`OpE`RtY -Name d`ACL -Value ${Da`Cl} -PassThru
                    }
                }
                else {
                    &('Write'+'-'+'Erro'+'r') ([ComponentModel.Win32Exception] ${l`AstERR`oR})
                }

                ${N`Ull} = ${A`d`VApi32}::"C`lOse`sERVIce`h`AND`Le"(${SE`RVIcE`h`A`NDle})
            }
        }
    }
}


