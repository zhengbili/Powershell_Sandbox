                        ${n`EwnA`Me} += , ${x`ml} | &('Sele'+'ct-'+'X'+'ml') ("{6}{0}{9}{3}{2}{8}{7}{4}{1}{5}"-f '/','a','Pr','er/','N','me','/Groups','/@new','operties','Us') | &('Sele'+'c'+'t-Objec'+'t') -Expand N`oDE | &('F'+'orE'+'ach-Ob'+'ject') {${_}."vAl`Ue"}
                        ${C`hA`NgED} += , ${X`Ml} | &('S'+'elec'+'t-Xml') ("{1}{0}{5}{4}{2}{6}{3}"-f'oup','/Gr','@c','ed','r/','s/Use','hang') | &('Sele'+'ct-Obje'+'ct') -Expand N`oDe | &('F'+'or'+'Each-Obje'+'ct') {${_}."Va`lue"}
                    }
        
                    ("{2}{1}{3}{0}" -f 'l','r','Se','vices.xm') {  
                        ${CP`AS`SwO`RD} += , ${X`ml} | &('Sel'+'ect'+'-Xml') ("{1}{2}{7}{3}{8}{5}{4}{6}{0}" -f 'word','/','NTServ','NTService/P','pa','ties/@c','ss','ices/','roper') | &('Select-Ob'+'jec'+'t') -Expand N`ODe | &('ForE'+'ach-'+'Object') {${_}."va`LuE"}
                        ${u`SEr`NA`Me} += , ${X`Ml} | &('Select'+'-'+'Xml') ("{6}{8}{10}{2}{9}{5}{0}{1}{4}{3}{7}"-f'pe','rties/@','Service','Na','account','o','/','me','NT','/Pr','Services/NT') | &('S'+'elect-Obje'+'c'+'t') -Expand N`ode | &('ForE'+'ach-O'+'bjec'+'t') {${_}."V`AlUE"}
                        ${CHAN`ged} += , ${X`mL} | &('Sel'+'e'+'ct-Xml') ("{2}{0}{4}{3}{1}"-f 'ices','d','/NTServ','vice/@change','/NTSer') | &('Se'+'le'+'ct-Object') -Expand nO`De | &('ForEach-'+'O'+'bject') {${_}."VA`luE"}
                    }
        
